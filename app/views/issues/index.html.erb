<nav>
  <div class="nav-wrapper blue darken-4">
    <img id="logo" alt="Icon" src="/assets/fist.png" />
    <ul id="nav-mobile" class="right hide-on-med-and-down">
      <li><%= link_to "Home", root_path %></li>
      <li><%= link_to "Login With Google", user_omniauth_authorize_path(:google_oauth2) %></li>
    </ul>
  </div>
</nav>

  <div class="parallax-container">
    <div class="parallax"><%= image_tag "/assets/Dec.jpg"%></div>
  </div>   


  <div class="container">
    <h1>Welcome To RESIST</h1>
    <h5>The crowdfunding platform for supporting the political causes that matter to you.</h5>
    <p> To Sign Or Donate Please Login </p>
    <div>
      <% @issues.each do |issue| %>

      <div class="col s12 m7">
        <h2 class="header"></h2>
        <div class="card horizontal blue darken-4">
          <div class="card-stacked">
            <div class="card-content">
              <h3><%= link_to issue.title, issue_path(issue) %></h3>
              <%= issue.description %>
            </div>
          </div>
        </div>
      </div>

      
      <% end %>
    </div>
  </div>


